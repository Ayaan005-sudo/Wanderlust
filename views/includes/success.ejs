




<%if(success && success.length){%>
 <div class="row ">
<div class="alert alert-success mt-2 mb-2 alert-dismissible fade show  col-8 offset-2" role="alert" style="padding: 1rem;">
  <%=success%> 
  <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
</div>
</div>
<%}%>


<% if(failure && failure.length){ %>
  <div class="row ">
    <div class="alert alert-danger mt-2 mb-2 alert-dismissible fade show  col-12 offset 3" role="alert" style="text-align: center;">
  <%=failure%> 
  <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
  </div>
  </div>

<%}%>